{
  "name": "@sockshop/performance",
  "version": "1.0.0",
  "description": "Performance testing (k6, Artillery)",
  "scripts": {
    "test:k6:petstore": "k6 run src/k6/petstore.load.js",
    "test:k6:petstore:smoke": "k6 run --env TEST_TYPE=smoke src/k6/petstore.load.js",
    "test:k6:petstore:load": "k6 run --env TEST_TYPE=load src/k6/petstore.load.js",
    "test:k6:petstore:stress": "k6 run --env TEST_TYPE=stress src/k6/petstore.load.js",
    "build": "webpack --config webpack.config.js",
    "lint": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "typescript": "^5.9.3",
    "@types/k6": "^0.54.0",
    "webpack": "^5.96.0",
    "webpack-cli": "^5.1.0",
    "copy-webpack-plugin": "^12.0.0"
  },
  "dependencies": {
    "@sockshop/shared": "1.0.0"
  }
}
